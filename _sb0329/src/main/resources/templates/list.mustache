{{>layout/header}}
<div>
    <!-- 목록 출력 영역-->
    <table class="tabel table-horizontal table-bordered">
        <thead class="thead-strong">
            <tr>
                <th>번호</th>
                <th>제품명</th>
                <th>가격</th>
            </tr>
        </thead>
        <tbody id="tbody">
            {{#list}}
            <tr>
                <td>{{prodId}}</td>
                <td><a href="/update/{{prodId}}">{{prodName}}</a></td>
                <td>{{prodPrice}}</td>
                <td><a href="/delete/{{prodId}}">삭제</a></td>
            </tr>
            {{/list}}         
     
        </tbody>
    </table>
    <div class="mb-2">
        <a href="/save" class="btn btn-success" role="button">신규</a>
       
    </div>
    
</div>
{{>layout/footer}}